<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>Microsoft.Bot.Builder.FormFlow.Json</AssemblyName>
    <RootNamespace>Microsoft.Bot.Builder.FormFlow.Json</RootNamespace>
    <PackageId>CXuesong.Ported.Microsoft.Bot.Builder.FormFlow.Json</PackageId>
    <Authors>Microsoft (Original), CXuesong (Ported)</Authors>
    <Company />
    <Product>Microsoft Bot Builder</Product>
    <Version>3.9.0-int0</Version>
    <Description>This is an unofficial ported version of Microsoft's Microsoft.Bot.Builder.FormFlow.Json package.</Description>
    <PackageProjectUrl>https://github.com/CXuesong/BotBuilder.Standard</PackageProjectUrl>
    <RepositoryUrl>https://github.com/CXuesong/BotBuilder.Standard</RepositoryUrl>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <GenerateDocumentationFiles>True</GenerateDocumentationFiles>
    <PackageReleaseNotes>See https://github.com/CXuesong/BotBuilder.Standard/releases .</PackageReleaseNotes>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.Scripting" Version="2.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Microsoft.Bot.Builder.Standard\Microsoft.Bot.Builder.Standard.csproj" />
    <ProjectReference Include="..\Microsoft.Bot.Connector.Standard\Microsoft.Bot.Connector.Standard.csproj" />
  </ItemGroup>

</Project>
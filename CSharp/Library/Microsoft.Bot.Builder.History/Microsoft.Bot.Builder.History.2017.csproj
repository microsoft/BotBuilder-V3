<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net46;netstandard2.0</TargetFrameworks>
    <AssemblyName>Microsoft.Bot.Builder.History</AssemblyName>
    <RootNamespace>Microsoft.Bot.Builder.History</RootNamespace>
    <PackageId>Microsoft.Bot.Builder.History</PackageId>
    <Product>Microsoft Bot Builder SDK</Product>
    <Version>3.0.6</Version>
    <Authors>Microsoft</Authors>
    <Description>
      Provide interfaces and classes for working with conversation histories in the Microsoft Bot Builder.
      In particular this includes facilities to walk across stored histories and replay them.
    </Description>
    <PackageProjectUrl>https://github.com/Microsoft/BotBuilder</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Microsoft/BotBuilder</RepositoryUrl>
    <PackageIconUrl>http://docs.botframework.com/images/bot_icon.png</PackageIconUrl>
    <RepositoryType>git</RepositoryType>
    <NeutralLanguage>en-US</NeutralLanguage>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <NoWarn>0419;1591;1701;1702;1705;1998</NoWarn>
  </PropertyGroup>
  
  <ItemGroup>
    <Compile Remove="Properties\**\*" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Rest.ClientRuntime" Version="2.3.2" />
    <PackageReference Include="Newtonsoft.Json" Version="9.0.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Microsoft.Bot.Connector\Microsoft.Bot.Connector.2017.csproj" />
    <ProjectReference Include="..\Microsoft.Bot.Builder\Microsoft.Bot.Builder.2017.csproj" />
  </ItemGroup>
  
</Project>